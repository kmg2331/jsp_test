	<% 
		ArticleVO article = (ArticleVO)request.getAttribute("articleVO");
		PrintWriter script = response.getWriter();
		TestUtils util = new TestUtils();
		Map<String, Object> map = util.toMap(article);
		
		script.println("<ul>");
		 for(Map.Entry<String, Object> elem : map.entrySet()){
			script.println("<li>");
			script.println("키 : " + elem.getKey() + " ------> 값 : " + elem.getValue());
			script.println("</li>");
         }
		script.println("</ul>");
